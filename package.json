{
  "name": "tabletrobotface-userstudy",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib && mkdirp lib",
    "build": "babel src -d lib",
    "prebrowserify": "rimraf dist && mkdirp dist",
    "browserify": "browserify src/index.js --standalone default -t babelify --outfile dist/index.js",
    "prepublishOnly": "npm run build && npm run browserify",
    "robot": "npm run --prefix ./apps/robot start",
    "prerobot": "[ `npm config get studyID` != undefined ] && json -I -f ./apps/data/studies/\"`npm config get studyID`\".json -e \"{this.settings.robot=`./apps/scripts/printsettings.js | json robot`}\"",
    "instructor": "npm run --prefix ./apps/instructor start",
    "dataplayer": "npm run --prefix ./apps/dataplayer start",
    "dataplayer": "if [ `npm config get studyID` != undefined ]; then json -I -f ./apps/data/studies/\"`npm config get studyID`\".json -e \"{this.settings.robot=`./apps/scripts/printsettings.js | json dataplayer`}\" ; fi",
    "createstudy": "./apps/scripts/createstudy.js | xargs -I{} npm config set studyID {}",
    "precreatestudy": "mkdirp ./apps/data/studies",
    "cpmostrecent": "./apps/scripts/cpmostrecent.sh",
    "createcorrections": "./apps/scripts/createcorrections.js \"`npm config get studyID`\"",
    "precreatecorrections": "mkdirp ./apps/data/corrections",
    "applysrtr": "DEBUG=applysrtr ./apps/scripts/applysrtr.js \"`npm config get studyID`\"| tee -a ~/tmp/applysrtr.log",
    "preapplysrtr": "mkdirp ~/tmp",
    "saveparams": "./apps/scripts/saveparams.js ./apps/data/studies/\"`npm config get studyID`\".json"
  },
  "author": "Michael Jae-Yoon Chung",
  "license": "MIT",
  "repository": "https://github.com/mjyc/tabletrobotface-userstudy.git",
  "dependencies": {
    "@cycle/dom": "22.3.0",
    "@cycle/isolate": "5.0.0",
    "@cycle/run": "5.2.0",
    "chart.js": "2.7.3",
    "chartjs-plugin-streaming": "1.7.1",
    "d3": "5.7.0",
    "dagre-d3": "0.6.3",
    "json": "^9.0.6",
    "xstream": "11.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "js2smt2": "file:../js2smt2",
    "loglevel-debug": "0.0.1",
    "mkdirp": "~0.5.0",
    "rimraf": "^2.6.3",
    "srtr": "file:../srtr"
  }
}
